{
  "kind": "collectionType",
  "collectionName": "apps",
  "info": {
    "singularName": "app",
    "pluralName": "apps",
    "displayName": "apps",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "photo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "description": {
      "type": "richtext"
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "apps"
    },
    "Owner": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::organisation.organisation",
      "inversedBy": "appsOwned"
    },
    "androidLink": {
      "type": "string"
    },
    "iosLink": {
      "type": "string"
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "app"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "apps"
    },
    "developer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::organisation.organisation",
      "inversedBy": "appsDeveloped"
    },
    "comment": {
      "type": "component",
      "repeatable": true,
      "component": "comments.comments"
    },
    "parent": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::organisation.organisation"
    },
    "screenshots": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
